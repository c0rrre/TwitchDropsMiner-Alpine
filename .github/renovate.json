{
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchManagers": [
        "pip_requirements"
      ],
      "prConcurrentLimit": 10
    },
    {
      "matchManagers": [
        "dockerfile"
      ],
      "prConcurrentLimit": 10,
      "commitMessagePrefix": "[docker] ",
      "reviewers": [
        "c0rrre"
      ]
    },
    {
      "matchManagers": [
        "github-actions"
      ],
      "prConcurrentLimit": 99,
      "commitMessagePrefix": "[github-action] ",
      "reviewers": [
        "c0rrre"
      ]
    }
  ]
}
